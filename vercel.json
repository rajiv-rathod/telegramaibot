{
  "version": 2,
  "functions": {
    "api/webhook.py": {
      "runtime": "@vercel/python"
    }
  },
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "OPENAI_ORG_ID": "@openai_org_id",
    "TELEGRAM_BOT_TOKEN": "@telegram_bot_token",
    "WEBHOOK_SECRET": "@webhook_secret"
  },
  "build": {
    "env": {
      "PYTHONPATH": "$PWD"
    }
  }
}